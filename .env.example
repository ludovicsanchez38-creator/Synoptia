# Configuration Synoptia Workflow Builder

# OpenAI API Configuration (OBLIGATOIRE)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic Claude API (OBLIGATOIRE - pour Supervisor Agent)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Cohere API (OBLIGATOIRE - pour RAG reranking)
COHERE_API_KEY=your-cohere-api-key-here

# n8n Configuration (OPTIONNEL - pour export automatique)
N8N_API_URL=https://your-n8n-instance.com/api/v1
N8N_API_KEY=your-n8n-api-key-here

# Server Configuration
PORT=3002
NODE_ENV=development

# Agent Configuration
AGENT_NAME=Synoptia Workflow Builder
AGENT_VERSION=1.0.0

# RAG System Configuration
QDRANT_URL=http://localhost:6333
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Feature Flags
ENABLE_RAG=true